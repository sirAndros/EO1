        </dt>

        <dd>
            @Html.DisplayFor(model => model.DisplayName)
        </dd>

        <dt>
            Тип
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ObjectType.Name)
        </dd>

        @*<dt>
            @Html.DisplayNameFor(model => model.ObjectInstance2.DisplayName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ObjectInstance2.DisplayName)
        </dd>*@

